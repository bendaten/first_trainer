<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<TrainingCenterDatabase xmlns="http://www.garmin.com/xmlschemas/TrainingCenterDatabase/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.garmin.com/xmlschemas/TrainingCenterDatabase/v2 http://www.garmin.com/xmlschemas/TrainingCenterDatabasev2.xsd">

  <Folders>
    <Workouts>
      <Running Name="My first marathon training plan">
        <WorkoutNameRef>
          <Id>Week 1 Key-run 1</Id>
        </WorkoutNameRef>
      </Running>
      <Biking Name="Biking"/>
      <Other Name="Other"/>
    </Workouts>
  </Folders>

  <Workouts>
    <Workout Sport="Running">
      <Name>Week 1 Key-run 1</Name>
      <Step xsi:type="Step_t">
        <StepId>0</StepId>
        <Name>Warm up</Name>
        <Duration xsi:type="Time_t">
          <Seconds>900</Seconds>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="Speed_t">
          <SpeedZone xsi:type="CustomSpeedZone_t">
          <LowInMetersPerSecond>2.6600727</LowInMetersPerSecond>
          <HighInMetersPerSecond>2.7047798</HighInMetersPerSecond>
        </SpeedZone>
        </Target>
      </Step>
      <Step xsi:type="Repeat_t">
        <StepId>1</StepId>
        <Name>repeat X 8</Name>
        <Repetitions>8</Repetitions>
        <Child xsi:type="Step_t">
          <StepId>2</StepId>
          <Name>Fast</Name>
          <Duration xsi:type="Distance_t">
            <Meters>400</Meters>
          </Duration>
          <Intensity>Active</Intensity>
          <Target xsi:type="Speed_t">
            <SpeedZone xsi:type="CustomSpeedZone_t">
            <LowInMetersPerSecond>3.3182351</LowInMetersPerSecond>
            <HighInMetersPerSecond>3.3880926</HighInMetersPerSecond>
          </SpeedZone>
          </Target>
        </Child>
        <Child xsi:type="Step_t">
          <StepId>3</StepId>
          <Name>Rest</Name>
          <Duration xsi:type="Distance_t">
            <Meters>400</Meters>
          </Duration>
          <Intensity>Active</Intensity>
          <Target xsi:type="Speed_t">
            <SpeedZone xsi:type="CustomSpeedZone_t">
            <LowInMetersPerSecond>2.6600727</LowInMetersPerSecond>
            <HighInMetersPerSecond>2.7047798</HighInMetersPerSecond>
          </SpeedZone>
          </Target>
        </Child>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>4</StepId>
        <Name>Cool down</Name>
        <Duration xsi:type="Time_t">
          <Seconds>600</Seconds>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="Speed_t">
          <SpeedZone xsi:type="CustomSpeedZone_t">
          <LowInMetersPerSecond>2.6600727</LowInMetersPerSecond>
          <HighInMetersPerSecond>2.7047798</HighInMetersPerSecond>
        </SpeedZone>
        </Target>
      </Step>
      <ScheduledOn>2017-06-24</ScheduledOn>
    </Workout>
  </Workouts>

</TrainingCenterDatabase>
